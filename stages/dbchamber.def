[info]
name ="Dragon Ball-Hyperbolic time chamber"
displayname="Dragon Ball-Hyperbolic time chamber"
versiondate=22/09/2012
mugenversion=04,14,2001
author="Daeron"
pal.defaults=1,2,3,4,5

[camera]
startx = -16
starty = 0
boundleft = -240
boundright = 208
boundhigh = -230
boundlow = 0
verticalfollow = .75
tension = 50
floortension 	= 90


;--------------------------------------------------------
[playerinfo]
p1startx = -86          ;starting x coordinates
p1starty = 0            ;starting y coordinates
p1startz = 0            ;starting z coordinates
p1facing = 1            ;direction player faces: 1=right, -1=left

p2startx = 54
p2starty = 0
p2startz = 0
p2facing = -1

leftbound  = -1000 ;left bound (x-movement)
rightbound =  1000 ;right bound
topbound  =  0   ;Top bound (z-movement)
botbound  =  0    ;Bottom bound

;--------------------------------------------------------
[scaling]
topz     = 0       ;top z-coordinate for scaling
botz     = 50      ;bottom z-coordinate for scaling
topscale = 1       ;scale to use at top
botscale = 1.2     ;scale to use at bottom

;--------------------------------------------------------
[bound]
screenleft = 15    ;dist from left of screen that player can move to
screenright = 15   ;right edge

;--------------------------------------------------------
[stageinfo]
 
zoffset = 220
autoturn = 1
resetbg = 0

;--------------------------------------------------------
[shadow]
intensity = 40
color = 30,20,20
fade.range = 0,-50
yscale = 0.4
;--------------------------------------------------------
[Reflection]
 ;Intensity of reflection (from 0 to 256). Set to 0 to have no
 ;reflection. Defaults to 0.
intensity = 0



;--------------------------------------------------------
[music]

bgmusic = sound/Eyeless.mp3
bgvolume = 800

