; Player information
[Info]
name = "Super Gotenks"        ;Name of character
displayname = "Gotenks SSJ3" ;Name of character to display
versiondate = 20,01,2013    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0          ;Version of M.U.G.E.N character works on (X.XX)
author = "Gladiacloud"      ;Character author name
pal.defaults = 1            ;Default palettes in order of preference (up to 4)
                            ;Numbering starts from 1
localcoord = 320,240        ;Local coordinate space width and height

; Files for the player
[Files]
cmd     = gotenks.cmd           ;Command set
cns     = gotenks.cns           ;Constants
st      = gotenks.cns           ;States
st1     = attack.cns
st2     = special.cns
st3     = hyper.cns
stcommon = common1.cns          ;Common states (from data/ or motif)
sprite  = gotenks.sff           ;Sprite
anim    = gotenks.air           ;Animation
sound   = gotenks.snd           ;Sound

; Maps character selection buttons to palette numbers.
[Palette Keymap]
x = 1 ;Press button X to select palette 1, etc.
y = 2
z = 3
a = 4
b = 5
c = 6

[Arcade]
intro.storyboard = 
ending.storyboard = final\final.def