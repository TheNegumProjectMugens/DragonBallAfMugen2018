[Info]
name = "Android 17"        ;Name of character
displayname = "Android 17" ;Name of character to display
versiondate = 28,02,2013    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0          ;Version of M.U.G.E.N character works on (X.XX)
author = "Gladiacloud"         ;Character author name
pal.defaults = 1 
localcoord = 320,240        ;Local coordinate space width and height

; Files for the player
[Files]
cmd     = android17.cmd           ;Command set
cns     = android17.cns           ;Constants
st      = android17.cns           ;States
st1     = attacks.cns
st2     = special.cns
st3     = hypers.cns
stcommon = common1.cns      ;Common states (from data/ or motif)
sprite  = android17.sff           ;Sprite
anim    = android17.air           ;Animation
sound   = android17.snd           ;Sound

[Palette Keymap]
x = 1 ;Press button X to select palette 1, etc.
y = 2
z = 3
a = 4
b = 5
c = 6