; Player information
[Info]
name = "Uub"        ;Name of character
displayname = "Majuub" ;Name of character to display
versiondate = 03,22,2013    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0          ;Version of M.U.G.E.N character works on (X.XX)
author = "Gladiacloud"         ;Character author name
pal.defaults = 1
localcoord = 320,240        ;Local coordinate space width and height

; Files for the player
[Files]
cmd     = uub.cmd           ;Command set
cns     = uub.cns           ;Constants
st      = uub.cns           ;States
st1     = attacks.cns
st2     = specials.cns
st3     = hypers.cns
st4     = misc.cns
stcommon = common1.cns      ;Common states (from data/ or motif)
sprite  = uub.sff           ;Sprite
anim    = uub.air           ;Animation
sound   = uub.snd           ;Sound
